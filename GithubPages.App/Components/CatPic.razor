<FluentCard Width="@(Width + "px")" Height="@(Height + "px")" Style="border:none;padding:10px;">
    @if (!IsLoaded)
    {
        <FluentSkeleton Shimmer="true"></FluentSkeleton>
    }
    else
    {
        <img src="data:image;base64, @System.Convert.ToBase64String(CatImage)" />
    }
</FluentCard>
